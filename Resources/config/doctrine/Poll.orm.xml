<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <mapped-superclass name="Bait\PollBundle\Entity\Poll">
        <field name="title" type="string" column="title" length="255" />
        <field name="createdBy" type="string" column="created_by" length="255" />
        <field name="createdAt" type="datetime" column="created_at" />
        <field name="modifiedBy" type="string" column="modified_by" length="255" />
        <field name="modifiedAt" type="datetime" column="modified_at" />
        <field name="startAt" type="datetime" column="start_at" />
        <field name="endAt" type="datetime" column="end_at" nullable="true" />
        <field name="isActive" type="boolean" column="is_active" />
        <field name="votesVisible" type="boolean" column="votes_visible" />
    </mapped-superclass>
</doctrine-mapping>
