<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <mapped-superclass name="Bait\PollBundle\Entity\Poll">
        <field name="createdBy" type="string" column="createdBy" length="255" />
        <field name="addedAt" type="datetime" column="addedAt" />
        <field name="startAt" type="datetime" column="startAt" />
        <field name="endAt" type="datetime" column="endAt" />
        <field name="isVisible" type="boolean" column="isVisible" />
        <field name="votesVisible" type="boolean" column="votesVisible" />
    </mapped-superclass>
</doctrine-mapping>
